<h2>Users Table</h2>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Zip Code</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
      <!--refers to users in the controller. Links to user array-->
    <tr>
      <td>
        <%= user.fullName %>
      </td>
      <td>
        <%= user.email %>
      </td>
      <td>
        <%= user.zipCode %>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
